auto a = 500 * 10 * 500;
print("a = ", a, "\n");

auto b = 'h';
print("b = ", b, "\n");

auto c = "hola" + "hola";
print("c = ", c, "\n");

auto d = true;
print("d = ", d, "\n");

auto e = false;
print("e = ", e, "\n");

auto f = 3.14;
print("f = ", f, "\n");

auto g = f3.14;
print("g = ", g, "\n");

auto h = null;
print("h = ", h, "\n");

/* Explicit types */

let a2: int = 500;
print("a2 = ", a2, "\n");

let b2: char = 'h';
print("b2 = ", b2, "\n");

let c2: str = "hola";
print("c2 = ", c2, "\n");

let d2: bool = true;
print("d2 = ", d2, "\n");

let e2: bool = false;
print("e2 = ", e2, "\n");

let f2: double = 3.14;
print("f2 = ", f2, "\n");

let g2: float = f3.14;
print("g2 = ", g2, "\n");

let h2: int* = null;
print("h2 = ", h2, "\n");